{$lua}
local al = getAddressList()

local ItemList = {}
local index = 0

ItemList[index+1] = "4294967295:Nothing"
index = index + 1

local BaseTextIDPtr = getAddress("ItemTextIDs")
local TotalItems = ReadInteger(getAddress("TotalItems"))
local CurrentTextIDItem = 0
while (CurrentTextIDItem &lt; TotalItems) do
      local CurrentTextID = ReadInteger(BaseTextIDPtr + (0x04 * CurrentTextIDItem))
      local BasePtrAddr = ReadPointer(getAddress("IndianaEpicGameStore-Win64-Shipping.exe") + 0x03D99AF8)
      if (BasePtrAddr ~= nil and BasePtrAddr ~= 0) then
         local Offset0Ptr = ReadPointer(BasePtrAddr + 0x218)
         if (Offset0Ptr ~= nil and Offset0Ptr ~= 0) then
            local Offset1Ptr = ReadPointer(Offset0Ptr + 0x60)
            if (Offset1Ptr ~= nil and Offset1Ptr ~= 0) then
               local Offset2Ptr = ReadPointer(Offset1Ptr + 0x1D8)
               if (Offset2Ptr ~= nil and Offset2Ptr ~= 0) then
                  local NumItems = 0
                  while (NumItems &lt; 2967) do
					local ItemTextID = ReadInteger(Offset2Ptr + (0x48 * NumItems) + 0x08)
					if (CurrentTextID == ItemTextID) then
					  local ItemText = ReadString(ReadPointer(Offset2Ptr + (0x48 * NumItems) + 0x10),500,true)
					  if (ItemText == nil) then
					     ItemText = ""
					  end
					  local ItemTextFinal = string.gsub(ItemText,"\n"," | ")
                      ItemList[index+1] = string.format('%d:%s',ItemTextID,ItemTextFinal)
                      break
					else
						if (ItemTextID &gt; CurrentTextID) then
						  NumItems = 2967
						  break
						end
                    end
                    NumItems = NumItems + 1
                  end
                  if (NumItems &gt;= 2967) then
                     ItemList[index+1] = string.format('%d:Unknown Item %d',CurrentTextID,CurrentTextID)
                  end
               end
            end
         end
      end
      CurrentTextIDItem = CurrentTextIDItem + 1
	  index = index + 1
end

local DropDownListITM = createStringlist()
for i,j in ipairs(ItemList) do
	DropDownListITM.add(string.format("%s",j))
end

