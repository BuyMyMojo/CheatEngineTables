local DropDownListITM = createStringlist()
for i,j in ipairs(ItemList) do
	DropDownListITM.add(string.format("%s",j))
end

local mr = al.getMemoryRecordByDescription("Make Console Command GivePlayerAllItems Only Give You Specific Category of Items")
al[mr.Index+1].DropDownList.Text = DropDownListITM.Text
al[mr.Index+1].DropDownReadOnly = false
al[mr.Index+1].DropDownDescriptionOnly = true
al[mr.Index+1].DisplayAsDropDownListItem = true

